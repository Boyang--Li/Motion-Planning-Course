<?xml version="1.0"?>
<package>
  <name>rosplan_interface_mapping</name>
  <version>0.0.2</version>
  <description>
    Interface between ROSPlan and a PRM approach (Probabilistic RoadMap) for waypoint generation.
    Generate valid waypoints from a given map or load them from file.
    Waypoints are stored symbolically in the Knowledge Base.
    Coordinates are stored in the parameter server.
  </description>

  <maintainer email="michael.cashmore@kcl.ac.uk">Michael Cashmore</maintainer>
  <maintainer email="oscar.lima@dfki.de">Oscar Lima</maintainer>

  <license>BSD</license>
  <url type="website">https://github.com/KCL-Planning/ROSPlan</url>

  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>roscpp</build_depend>
  <build_depend>diagnostic_msgs</build_depend>
  <build_depend>geometry_msgs</build_depend>
  <build_depend>nav_msgs</build_depend>
  <build_depend>std_srvs</build_depend>
  <build_depend>visualization_msgs</build_depend>
  <build_depend>rosplan_knowledge_msgs</build_depend>
  <build_depend>tf</build_depend>
  <build_depend>occupancy_grid_utils</build_depend>
  <build_depend>message_generation</build_depend>
  <build_depend>genmsg</build_depend>

  <run_depend>roscpp</run_depend>
  <run_depend>diagnostic_msgs</run_depend>
  <run_depend>geometry_msgs</run_depend>
  <run_depend>nav_msgs</run_depend>
  <run_depend>std_srvs</run_depend>
  <run_depend>visualization_msgs</run_depend>
  <run_depend>rosplan_knowledge_msgs</run_depend>
  <run_depend>tf</run_depend>
  <run_depend>occupancy_grid_utils</run_depend>
  <run_depend>message_generation</run_depend>
  <run_depend>message_runtime</run_depend>
  <run_depend>genmsg</run_depend>

</package>
